        <!--Playlist-->
        <div class="container-fluid cardcontainer dropdown row-eq-height">

            <div class="col-sm-8 no_pad exercises">
                <div class="col-sm-12 container_card template_padding2">
                    <!--Workout Exercises-->
                    <h2 class = "template_padding col-sm-12">Exercises</h2>
                    <div class="exercise-layout">
                        <!--<form action = "\newExercise">-->
                        <!--Exercises-->
                        <div class="form-group col-sm-12">
                            <label for="exercise">Exercise</label>
                            <input name="exercise_name" type="search" class="form-control input-text"
                                   id="exercise" placeholder="Enter exercise">
                        </div>
                        <!--Sets-->
                        <div class="form-group col-sm-12">
                            <label for="set">Sets</label>
                            <input type="number" class="form-control input-number" id="set">
                        </div>
                        <!--Repetitions-->
                        <div class="form-group col-sm-12">
                            <label for="repetition">Repetitions</label>
                            <input type="number" class="form-control input-number" id="repetition">
                        </div>
                        <!--Rest-->
                        <div class="form-group col-sm-12">
                            <label for="rest">Rest (in seconds)</label>
                            <input type="number" class="form-control input-number" id="rest">
                        </div>
                        <!--Add to workout-->
