    <div class = " container-fluid " id = "page-content-wrapper">

        <div class = "container-fluid basic_card dropdown template_padding">
            <div class = "major_title"> <b>Workout Builder</b> </div>
        </div>
        <div class = "container-fluid basic_card dropdown">
            <h2 class = "template_padding col-sm-12">Workout Details</h2>
            <div class="form-layout">
                <!--<form action = "/newWorkout" method = "post">-->
                    <!--Gender-->
                <div class="col-sm-12 form-group">
                        <div>
                            <label for="genderToggle">Gender</label>
                        </div>
                    <div class="btn-group-toggle" data-toggle="buttons" id="genderToggle">
                <label class="btn btn-default active">
                    <input type="radio" name="options" id="male" autocomplete="off" checked> Male
                </label>
                <label class="btn btn-default">
                    <input type="radio" name="options" id="female" autocomplete="off"> Female
                </label>
                <label class="btn btn-default">
                    <input type="radio" name="options" id="any-gender" autocomplete="off"> Any
                </label>
            </div>
        </div>
                    <!--Name-->
                    <div class="form-group col-sm-12">
                        <label for="tag">Workout name</label>
                        <input name = "workoutName" type="search" class="form-control input-text"
                               id="workoutName" placeholder="Enter workout name (Each workout name must be unique)">
                    </div>
                    <!--Time-->
                    <div class="form-group col-sm-12">
                        <label for="duration">Duration (in minutes)</label>
                        <input name = "duration" type="search" class="form-control input-text" id="duration" placeholder = "Enter Duration">
                    </div>
                    <!--Tags-->
                    <div class="form-group col-sm-12">
                        <label for="tag">Tags</label>
                        <input name = "tags" type="search" class="form-control input-text"
                               id="tag" placeholder="Enter or search workout tags">
                    </div>
                    <!--Muscle Groups-->
                    <div class="form-group col-sm-12">
                        <label for="muscle-group">Muscle Groups</label>
                        <input name = "muscleGroups" type="search" class="form-control input-text" id="muscle-group"
                               placeholder="Enter or search muscle groups">
                    </div>
                    <!--Equipment-->
                    <div class="form-group col-sm-12">
                        <label for="Equipment">Equipment</label>
                        <input name = "equipment" type="search" class="form-control input-text" id="equipment"
                               placeholder="Enter or search equipment">
                    </div>
                    <!--Intensity-->
                    <div class="col-sm-12 form-group">
                        <div>
                            <label for="intensityToggle">Intensity</label>
                        </div>
                        <div class="btn-group-toggle" data-toggle="buttons" id="intensityToggle">
                            <label class="btn btn-default active">
                                <input type="radio" name="intensity" id="low" autocomplete="off" checked> Low
                            </label>
                            <label class="btn btn-default">
                                <input type="radio" name="intensity" id="moderate" autocomplete="off"> Moderate
                            </label>
                            <label class="btn btn-default">
                                <input type="radio" name="intensity" id="vigorous" autocomplete="off"> Vigorous
                            </label>
                        </div>
                    </div>
                    <!--Difficulty-->
                    <div class="col-sm-12 form-group">
                        <div>
                            <label for="difficultyToggle">Difficulty</label>
                        </div>
                        <div class="btn-group-toggle" data-toggle="buttons" id="difficultyToggle">
                            <label class="btn btn-default active">
                                <input type="radio" name="difficulty" id="beginner" autocomplete="off" checked> Beginner
                            </label>
                            <label class="btn btn-default">
                                <input type="radio" name="difficulty" id="intermediate" autocomplete="off"> Intermediate
                            </label>
                            <label class="btn btn-default">
                                <input type="radio" name="difficulty" id="advanced" autocomplete="off"> Advanced
                            </label>
                        </div>
                    </div>
                    <!--Workout Description-->
                    <div class="form-group col-sm-12">
                        <label for="workoutText">Workout Description</label>
                        <textarea name = "workoutDescription" class="form-control input-text" id="aboutText" rows="3"></textarea>
                    </div>
                <!--</form>-->
            </div>
        </div>
